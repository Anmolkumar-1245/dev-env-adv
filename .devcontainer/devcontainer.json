{
    "name": "Dev Environment - Node.js + MySQL + Redis",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "backend",
    "workspaceFolder": "/app",
    "settings": {
      "terminal.integrated.defaultProfile.linux": "bash"
    },
    "extensions": [
      "dbaeumer.vscode-eslint",
      "ms-python.python",
      "ms-azuretools.vscode-docker"
    ],
    "forwardPorts": [3000],
    "postCreateCommand": "npm install",
    "remoteUser": "root"
  }
  